# Ignore Python cache files
__pycache__/
*.py[cod]
*$py.class

# Ignore environment files
.env
.venv
env/
venv/

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore Docker files
.dockerignore

# Ignore generated images (optional - remove if you want to track them)
generated_images/*.png
generated_images/*.jpg
generated_images/*.jpeg

# Keep generated_images directory
!generated_images/.gitkeep

# Ignore logs
*.log
logs/

# Ignore temporary files
*.tmp
*.temp